{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "jsx": "preserve",
        "strictFunctionTypes": true,
        "sourceMap": true,
        "outDir": "ts_compile",
        "lib": ["dom", "es2018"],
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ],
    "include": [
        "script/*",
    ]
}